<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="bcs.csc411.csc411project.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Welcome to App">
        <font>
            <Font size="24.0" />
        </font></Label>

    <Label fx:id="welcomeText"/>
    <Button mnemonicParsing="false" onAction="#switchToLogin" text="Sign in" />
    <Button mnemonicParsing="false" onAction="#switchToSignUp" text="Sign up" />
</VBox>
